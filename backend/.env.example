ENVIRONMENT=local
DATABASE_URL=postgresql+psycopg2://app:app@db:5432/app

# LiveKit Cloud: create a project at https://cloud.livekit.io
# Then copy from Project Settings: URL (wss://...), API Key, API Secret.
# Leave LIVEKIT_PUBLIC_URL unset to use LIVEKIT_URL for both server and frontend.
LIVEKIT_URL=wss://your-project.livekit.cloud
LIVEKIT_API_KEY=your_api_key
LIVEKIT_API_SECRET=your_api_secret
# Optional: URL to give to browser clients (e.g. if backend uses internal URL)
# LIVEKIT_PUBLIC_URL=wss://your-project.livekit.cloud

OPENAI_API_KEY="OPENAI_API_KEY"